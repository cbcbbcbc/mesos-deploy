{
  "id": "/elasticsearch",
  "cmd": null,
  "cpus": 0.2,
  "mem": 512,
  "disk": 0,
  "instances": 1,
  "acceptedResourceRoles": [
    "*"
  ],
  "container": {
    "type": "DOCKER",
    "volumes": [],
    "docker": {
      "image": "mesos/elasticsearch-scheduler",
      "network": "BRIDGE",
      "portMappings": [
        {
          "containerPort": 31100,
          "hostPort": 0,
          "servicePort": 10005,
          "protocol": "tcp",
          "labels": {}
        }
      ],
      "privileged": false,
      "parameters": [],
      "forcePullImage": false
    }
  },
  "env": {
    "JAVA_OPTS": "-Xms128m -Xmx256m"
  },
  "portDefinitions": [
    {
      "port": 10005,
      "protocol": "tcp",
      "name": "default",
      "labels": {}
    }
  ],
  "args": [
    "--zookeeperMesosUrl",
    "zk://mesos.service.consul:2181/mesos"
  ]
}